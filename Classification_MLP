      "cell_type": "code",
      "source": [
        "S = recall_score(y_test,pred,pos_label=1)\n",
        "E = recall_score(y_test,pred,pos_label=0)\n",
        "\n",
        "beta = 0.7\n",
        "wtdAcc = (beta*E) + ((1-beta) * S) \n",
        "wtdAcc"
